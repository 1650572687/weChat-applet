/* components/Tabs/Tabs.wxss */
.tabs .title {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15rpx 0;
}
.tabs .title .title_item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15rpx 0;
}
.tabs .title .active {
  color: var(--themeColor);
  border-bottom: 5rpx solid var(--themeColor);
}
